{
  "name": "@cph-scorer/starter-client",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "serve": "sirv dist -c",
    "lint": "js-standard --report stylish --plugin svelte3 --fix src",
    "prettier": "prettier --write --plugin-search-dir=. .",
    "format": "pnpm run lint && pnpm run prettier",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@popperjs/core": "^2.11.2",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^27.0.6",
    "bootstrap": "^5.1.3",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "jest": "^27.0.6",
    "js-standard": "^1.0.1",
    "node-sass": "^7.0.1",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rollup": "^2.56.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.7",
    "svelte": "^3.42.3",
    "svelte-jester": "^1.7.0",
    "svelte-preprocess": "^4.10.2"
  },
  "dependencies": {
    "single-spa-css": "^2.0.0",
    "single-spa-svelte": "^2.1.1",
    "sirv-cli": "^1.0.14"
  }
}
